<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dms.dao.IAdminDao">

	<select id="findOne" resultType="Admin">
		select * from admin where email=#{email} and password=#{password}
	</select>
	
	<select id="findEmailById">
		select email from admin where id=#{id}
	</select>
	
	<select id="findIdByEmail">
		select id from admin where email=#{email}
	</select>
	
	<insert id="insertAdmin" keyProperty="Id" parameterType="Admin">
		insert into admin() values(email,password) values(#{email},#{password})
	</insert>

</mapper>