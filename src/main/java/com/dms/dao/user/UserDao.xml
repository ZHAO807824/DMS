<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dms.dao.user.IUserDao">

	<insert id="insertUser" keyProperty="Id" parameterType="User">
		insert into user(full_name,house_name,gender) values(#fullName,#houseName,#gender)
	</insert>
	
	<insert id="insertUserAdmin">
		insert into user_admin values(#{userId},#{adminId})
	</insert>
	
</mapper>